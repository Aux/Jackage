<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Jackage.Views.JackboxControl">

	<Button Padding="5" Width="132" Height="176" HorizontalAlignment="Left" VerticalAlignment="Top">
		<Panel ToolTip.Tip="Not Yet Supported">
			<Image Source="{Binding CoverArt}" />
			<PathIcon Width="50" Height="50" Data="{StaticResource lock_multiple_regular}" />
			<Panel HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Black" Opacity="100"/>
		</Panel>
	</Button>
	
</UserControl>
